{
  "$schema": "../Schema/pipeline.schema.json",
  "source": [
    {
      "Type": "sql",
      "ConnectionName": "sql",
      "TableName": "data",
      "BatchSize": 1000
    }
  ],
  "target": [
    {
      "Type": "csv",
      "FilePath": "..\\..\\..\\EtlApp\\TestFiles",
      "FilePrefix": "generated_data",
      "Delimiter": ",",
      "UpdateStrategy": "ReplaceComplete"
    }
  ],
  "mapping": {
    "Mappings": [
      {
        "SourceName": "Column_1",
        "Transform": [
          "replace"
        ]
      }
    ]
  },
  "transformer": [
    {
      "Type": "regex_replace",
      "Name": "replace",
      "ReplaceRegex": "1",
      "SelectRegex": "1"
    }
  ]
}